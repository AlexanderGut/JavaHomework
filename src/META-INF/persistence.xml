    <class>datos.Provincia</class>
    <class>datos.Estadoetapa</class>
    <class>datos.Estadoproyecto</class>
    <class>datos.Detallehora</class>
    <class>datos.TipoProyecto</class>
    <class>datos.Defecto</class>
    <class>datos.Privilegio</class>
    <class>datos.Comentario</class>
    <class>datos.Empleado</class>
    <class>datos.Proyecto</class>
    <class>datos.Rol</class>
    <class>datos.Testcase</class>
    <class>datos.Telefono</class>
    <class>datos.Comuna</class>
    <class>datos.Recurso</class>
    <class>datos.Etapa</class>
    <class>datos.Cliente</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/prueba3?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
